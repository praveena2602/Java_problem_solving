mport java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int prod=1,c;
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        while(n>0){
            c=n%10;
            prod=prod*c;
            n=n/10;
        }
        System.out.println(prod);
    }
}
